{
  "name": "openmanus",
  "version": "0.1.0",
  "description": "OpenManus TypeScript - Modular Agentic System for AI",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "ts-node src/index.ts",
    "dev:debug": "node --inspect -r ts-node/register src/index.ts",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write \"src/**/*.ts\"",
    "fix-types": "./fix-types.sh",
    "server": "ts-node src/server.ts",
    "dev:server": "nodemon --exec ts-node src/server.ts",
    "test:search": "ts-node src/test-search.ts",
    "test:browser": "ts-node src/test-browser.ts",
    "test:multi-agent": "ts-node src/test-multi-agent.ts",
    "test:viz": "ts-node src/test-visualization.ts",
    "test:planning": "ts-node src/test-structured-planning.ts",
    "test:travel-planner": "ts-node src/test-travel-planner.ts",
    "debug:browser": "ts-node src/debug-browser-agent.ts",
    "debug:browser:watch": "nodemon --watch src --ext ts --exec ts-node src/debug-browser-agent.ts",
    "test:terminate": "ts-node src/test-terminate-tool.ts"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "ai",
    "agent",
    "llm",
    "openai",
    "typescript",
    "mcp"
  ],
  "author": "OpenManus Contributors",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.777.0",
    "@browser-use/browser-use-node": "npm:@jsr/browser-use__browser-use-node@^0.1.0",
    "axios": "^1.6.2",
    "browser-use-node": "^0.1.11",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "duck-duck-scrape": "^2.2.7",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "google-it": "^1.6.4",
    "googlethis": "^1.8.0",
    "gpt-tokenizer": "^2.9.0",
    "openai": "^4.20.0",
    "pino": "^8.15.0",
    "pino-pretty": "^10.2.0",
    "playwright": "^1.51.1",
    "puppeteer": "^21.5.2",
    "pythonia": "^1.2.2",
    "toml": "^3.0.0",
    "tsc-watch": "^6.2.1",
    "turndown": "^7.1.2",
    "uuid": "^9.0.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.10.0",
    "@types/pino": "^7.0.5",
    "@types/puppeteer": "^7.0.4",
    "@types/turndown": "^5.0.4",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.46.0",
    "jest": "^29.6.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
